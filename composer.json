{
    "name": "otezvikentiy/json-rpc-api",
    "description": "Symfony Json RPC API bundle",
    "version": "1.0.8",
    "stability": "stable",
    "minimum-stability": "stable",
    "keywords": ["symfony", "bundle"],
    "type": "symfony-bundle",
    "license": "MIT",
    "support": {
        "issues": "https://gitflic.ru/project/otezvikentiy/json-rpc-api/issue"
    },
    "autoload": {
        "psr-4": {
            "OV\\JsonRPCAPIBundle\\": "src/",
            "OV\\JsonRPCAPIBundle\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=8.1",
        "doctrine/annotations": "^2.0",
        "symfony/framework-bundle": "^6.0",
        "symfony/yaml": "^6.0",
        "symfony/validator": "^6.0",
        "symfony/serializer": "^6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.3"
    },
    "authors": [
        {
            "name": "l.groshev",
            "email": "OtezVikentiy@gmail.com",
